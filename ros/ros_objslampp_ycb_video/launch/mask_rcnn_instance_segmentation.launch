<launch>

  <arg name="GUI" default="true" />

  <group ns="/camera">
    <node name="mask_rcnn_instance_segmentation"
          pkg="ros_objslampp_ycb_video" type="mask_rcnn_instance_segmentation.py"
          clear_params="true">
      <remap from="~input" to="color/image_rect_color" />
    </node>
    <node name="draw_instance_segmentation"
          pkg="ros_objslampp_ycb_video" type="draw_instance_segmentation.py"
          clear_params="true">
      <remap from="~input/rgb" to="color/image_rect_color" />
      <remap from="~input/label_ins" to="mask_rcnn_instance_segmentation/output/label_ins" />
      <remap from="~input/class" to="mask_rcnn_instance_segmentation/output/class" />
    </node>
    <group ns="draw_instance_segmentation">
      <node name="image_view"
            pkg="image_view" type="image_view"
            clear_params="true">
        <remap from="image" to="output" />
        <rosparam subst_value="true">
          gui: $(arg GUI)
        </rosparam>
      </node>
    </group>
  </group>

</launch>
